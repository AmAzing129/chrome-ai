{
  "name": "chrome-ai",
  "description": "Vercel AI provider for Chrome built-in model (Gemini Nano)",
  "version": "1.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --tsconfig=tsconfig.tsup.json --watch",
    "build": "tsup --tsconfig=tsconfig.tsup.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "ui:add": "shadcn-ui add --overwrite",
    "demo": "next dev",
    "lint": "next lint",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "author": "jeasonstudio <me@cowpoke.cc>",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^0.0.10",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "debug": "^4.3.5"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^1.6.0",
    "ai": "^3.1.31",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "lucide-react": "^0.383.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "shadcn-ui": "^0.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.1.0",
    "typescript": "^5",
    "vercel": "^34.2.5",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  }
