{
  "name": "chrome-ai",
  "description": "Vercel AI provider for Chrome built-in model (Gemini Nano)",
  "version": "1.9.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./polyfill": {
      "import": "./dist/polyfill.mjs",
      "require": "./dist/polyfill.js",
      "types": "./dist/polyfill.d.ts"
    }
  },
  "sideEffects": [
    "./dist/polyfill.global.js",
    "./dist/polyfill.js",
    "./dist/polyfill.mjs"
  ],
  "scripts": {
    "dev": "tsup --tsconfig=tsconfig.tsup.json --watch",
    "build": "tsup --tsconfig=tsconfig.tsup.json --clean",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --run",
    "ui:add": "shadcn-ui add --overwrite",
    "demo": "next dev",
    "lint": "next lint",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "release:prepare": "npm run lint && npm run test:ci && npm run build",
    "release": "npm run release:prepare && changeset version && changeset publish"
  },
  "files": [
    "dist"
  ],
  "author": "jeasonstudio <me@cowpoke.cc>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/provider": "^0.0.11",
    "@mediapipe/tasks-genai": "^0.10.14",
    "@mediapipe/tasks-text": "^0.10.14",
    "debug": "^4.3.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.6",
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/debug": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^1.6.0",
    "ai": "^3.2.16",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "lucide-react": "^0.383.0",
    "marked": "^13.0.1",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "shadcn-ui": "^0.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.1.0",
    "typescript": "^5",
    "vaul": "^0.9.1",
    "vercel": "^34.2.5",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  }
}
